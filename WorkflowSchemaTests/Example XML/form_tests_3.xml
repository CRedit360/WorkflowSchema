<workflow>
    <screen>
        <title>Results</title>
        <navigationItem>
            <hidesBackButton>YES</hidesBackButton>
            <rightBarButtonItem>
                <barButtonItem>
                    <title>Done</title>
                    <message>doneButtonTapped</message>
                </barButtonItem>
            </rightBarButtonItem>
        </navigationItem>
        <container>
            <label>Animal:</label>
            <label><string keyPath="input1" /></label>
            <label>Snack:</label>
            <label><string keyPath="input2" /></label>
            <label>Fruit:</label>
            <label><string keyPath="input3" /></label>
        </container>
        <sendMessage name="doneButtonTapped">
            <messageTarget>screen</messageTarget>
            <messageName>form3done</messageName>
        </sendMessage>
    </screen>
</workflow>
